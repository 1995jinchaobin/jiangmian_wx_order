.searchBar{
  position: relative;
  padding-top: 34rpx;
  padding-bottom: 72rpx;
  background: #fff;
}
.searchBar view:nth-child(1){
  width: 595rpx;
  height: 70rpx;
  display: flex;
  align-items: center;
  margin-left: 32rpx;
  background: #F5F5F5;
  border-radius: 2px;
}
.searchBar image{
  width: 28.2rpx;
  height: 28rpx;
  margin-left: 24.2rpx;
  margin-right: 12.2rpx;
}
.searchBar input{
  width: 367rpx;
  height: 40rpx;
  font-size: 28rpx;
}
.searchBar view:nth-child(2){
  width: 64rpx;
  height: 70rpx;
  font-size: 32rpx;
  color: #333333;
  text-align: center;
  line-height: 70rpx;
  display: inline-block;
  position: absolute;
  top: 34rpx;
  right: 32rpx;
}
.itemBox{
  width: 100%;
  height: 298rpx;
  background: #fff;
}
.userName view{
  width: 100%;
  height: 88rpx;
  background: #fff;
  font-size: 34rpx;
  color: #000000;
  line-height: 88rpx;
  border-bottom: 1rpx solid#C8C7CC;
  padding-left: 32rpx;
}
.userNameTop{
  border-top: 1rpx solid#C8C7CC;
}
.addBtn{
  width: 100%;
  height: 94rpx;
  background: #FFE300;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0;
}
.addBtn image{
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}
.addBtn text{
  font-size: 36rpx;
  color: #333333;
  line-height: 94rpx;
}
.userNameBox{
  margin-bottom: 94rpx;
}