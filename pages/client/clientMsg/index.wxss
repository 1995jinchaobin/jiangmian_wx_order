page{
  background: #F4F4F4;
}
.msgBox{
  width: 100%;
  height: 252rpx;
  background: #fff;
  display: flex;
  margin-bottom: 22rpx;
}
.msgBox image{
  width: 84rpx;
  height: 84rpx;
  margin: 54rpx 30rpx 114rpx 22rpx;
}
.msgBox .title{
  width: 428rpx;
  font-size: 42rpx;
  color: #333333;
  margin-top: 39rpx;
  margin-bottom: 22rpx;
}
.msgBox .name{
  font-size: 28rpx;
  color: #999999;
}
.detailBox{
  width: 100%;
  height: 396rpx;
  background: #fff;
}
.detailBox view{
  width: 100%;
  height: 100rpx;
  font-size: 28rpx;
  color: #333333;
  line-height: 100rpx;
  padding-left: 36rpx;
}
.addBtn{
  width: 100%;
  height: 94rpx;
  background: #FFE300;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  bottom: 0;
}
.addBtn image{
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}
.addBtn text{
  font-size: 36rpx;
  color: #333333;
  line-height: 94rpx;
}