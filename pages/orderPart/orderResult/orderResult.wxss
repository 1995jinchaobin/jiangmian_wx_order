/* pages/masks/orderResult/orderResult.wxss */
.orderResult{
  position: relative;
}
.header{
  display: flex;
  align-items: center;
  background-color:  rgba(237,237,237,0.90);
  height: 88rpx;
  margin-bottom: 26rpx;
}
.header .toHome{
  width: 80rpx;
  height: 88rpx;
  position: absolute;
}
.header .title{
  text-align: center;
  font-size: 34rpx;
  color: #000000;
  letter-spacing: 0;
  width: 100%;
}
.resultImgArea{
  width: 100%;
  display: flex;
  justify-content: center;
  margin-top: 126rpx;
}
.resultImg{
  width: 130rpx;
  height: 130rpx;
}
.resultTitle{
  width: 100%;
  font-size: 40rpx;
  color: #333333;
  letter-spacing: 0;
  text-align: center;
  margin-top: 32rpx;
}
.resultDetail{
  font-size: 28rpx;
  color: #999999;
  letter-spacing: 0;
  text-align: center;
  width: 100%;
  margin-top: 16rpx;
}
.btnsArea{
  width: 100%;
  margin-top: 120rpx;
}
.backBtn{
  margin: 0 auto;
  width: 686rpx;
  height: 94rpx;
  line-height: 94rpx;
  text-align: center;
  background: #FFE300;
  border-radius: 4rpx;
  font-size: 36rpx;
  color: #333333;
}
.myOrderBtn{
  margin: 36rpx auto;
  width: 686rpx;
  height: 94rpx;
  line-height: 94rpx;
  text-align: center;
  background: #FFFFFF;
  border: 1rpx solid #D9D9D9;
  border-radius: 4rpx;
  font-size: 36rpx;
  color: #999999;
}
.bottomDetail{
  font-size: 24rpx;
  color: #CCCCCC;
  letter-spacing: 0.58rpx;
  text-align: center;
  position: absolute;
  bottom: 47rpx;
  width: 100%;
}