page{
  background: #F4F4F4;
}
.flex{
  display: flex;
}
.flexAli{
  display: flex;
  align-items: center;
}
.msgBox{
  width: 100%;
  height: 252rpx;
  background: #fff;
  display: flex;
  margin-bottom: 20rpx;
}
.msgBox image{
  width: 84rpx;
  height: 84rpx;
  margin: 54rpx 30rpx 114rpx 22rpx;
}
.msgBox .title{
  width: 428rpx;
  font-size: 42rpx;
  color: #333333;
  margin-top: 39rpx;
  margin-bottom: 22rpx;
}
.msgBox .name{
  font-size: 28rpx;
  color: #999999;
}
.detailBox{
  width: 100%;
  height: 396rpx;
  background: #fff;
}
.detailBox view{
  width: 100%;
  height: 100rpx;
  font-size: 28rpx;
  color: #333333;
  line-height: 100rpx;
  padding-left: 36rpx;
}
.flowerBox{
  width: 100%;
  height: 226rpx;
  background: #FFFFFF;
  padding-top: 40rpx;
  position: relative;
  margin-bottom: 16rpx;
}
.flowerBox .msgBox{
  position: absolute;
  width: 83%;
  height: 192rpx;
  /* display: flex; */
  white-space: nowrap;
  top: 40rpx;
  left: 124rpx;
}
.flowerBox .title{
  font-size: 34rpx;
  color: #333333;
  margin-left: 30rpx;
}
.scrollItem{
  display: inline-block;
  position: relative;
  width: 108rpx;
}
.scrollItem+.scrollItem{
  margin-left: 28rpx;
}

.flowerBox .msgBox image{
  width: 120rpx;
  height: 120rpx;
  position: relative;
  top: 10rpx;
  margin: 0;
}
.placeMsg{
  width: 120rpx;
  font-size: 24rpx;
  color: #333333;
  text-align: center;
  position: relative;
  overflow: hidden;
  white-space:normal;
  word-break:break-all;
}
.fromBox{
  background: #FFFFFF;
}
.fromBox view{
  padding-left: 32rpx;
  margin-bottom: 16rpx;
}
.fromBox view text:nth-child(1){
  font-size: 28rpx;
  color: #999999;
  text-align: left;
}
.fromBox view text:nth-child(2){
  position: absolute;
  right: 32rpx;
  font-size: 28rpx;
  color: #333333;
  text-align: right;
}
.maxBox{
  width: 750rpx;
  height: 100vh;
  background: rgb(0, 0, 0);
  position: fixed;
  top: 0;
}
.maxImgBox{
  width: 750rpx;
  height: 70%;
  overflow: hidden;
  position: relative;
  top: 15%;
}
.maxImgBox image{
  width: 750rpx;
}
.btnBox{
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: center;
}
.btn{
  width: 176rpx;
  height: 64rpx;
  border-radius: 8rpx;
  text-align: center;
  line-height: 64rpx;
  font-size: 28rpx;
  margin-top: 42rpx;
  background: #fff;
}
.btn:first-child{
  background: #1890FF;
  color: #fff;
  margin-right: 16rpx;
}
.operateArea{
  width: 670rpx;
  margin:0 auto;
  padding-bottom: 200rpx;
}
.operateTitle{
  display: flex;
  margin-bottom: 84rpx;
}
.operateLeftTitle{
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  color: #132051;
  letter-spacing: -0.7rpx;
}
.leftBigCircle{
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  border: 4rpx solid rgba(139,145,165,0.30);
  padding: 4rpx;
}
.leftSmallCircle{
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background-color:  rgba(139,145,165,0.30);
}
.orderState{
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #132051;
  letter-spacing: -0.56rpx;
  margin-left: 28rpx;
}
.leftLine{
  height: 120rpx;
  border: 2rpx solid rgba(139,145,165,0.30);
  border-radius: 100px;
  margin-left: 12rpx;
}
.operateUser{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #8D93AC;
  letter-spacing: -0.44rpx;
  margin-left: 40rpx;
  margin-top: 8rpx;
}
.operateTime{
  font-family: Roboto-Regular;
  font-size: 28rpx;
  color: #8D93AC;
  letter-spacing: -0.44rpx;
  text-align: right;
  float: right;
  margin-top: 8rpx;
  position: absolute;
  right: 40rpx;
}
.maxFlowerName{
  color: #fff;
  position: absolute;
  top: 5%;
  width: 100%;
  overflow: hidden;
  white-space:normal;
  word-break:break-all;
}
.flowerBox{
  width: 100%;
  height: 226rpx;
  background: #FFFFFF;
  padding-top: 40rpx;
  position: relative;
}
.madan{
  display: flex;
}
.madan text{
  flex: 1;
}
.madanImage,.madanImage image{
  width: 150rpx;
  margin-right: 30rpx;
}
.printMeterBg{
  width: 100%;
  opacity: 0.75;
  background: #000000;
  position: fixed;
  top: 0px;
  left: 0px;
}
.printMeterArea{
  width: 540rpx;
  height: 476rpx;
  border-radius: 4rpx;
  position: fixed;
  left: 105rpx;
  background-color: #ffffff;
  opacity: 1;
}
.printMeterTitle{
  font-size: 34rpx;
  color: #333333;
  margin-top: 43rpx;
  margin-left: 32rpx;
  margin-right: 32rpx;
  display: flex;
}
.printBtnArea{
  height: 100rpx;
  display: flex;
  position: absolute;
  bottom: 0px;
  width: 100%;
}
.cancelBtn{
  width: 50%;
  height: 100%;
  line-height: 100rpx;
  text-align: center;
  border-top: 1rpx solid #DDDDDD;
  border-right: 1rpx solid #DDDDDD;
  font-size: 36rpx;
  color: #999999;
  letter-spacing: 0.58rpx;
}
.sureBtn{
  width: 50%;
  height: 100%;
  line-height: 100rpx;
  text-align: center;
  border-top: 1rpx solid #DDDDDD;
  border-right: 1rpx solid #DDDDDD;
  font-size: 36rpx;
  color: #108EE9;
  letter-spacing: 0.58rpx;
}
.picker{
  width: 100%;
  height: 150rpx;
  font-size: 44rpx;
  text-align: center;
  margin-top: 50rpx;
}
