        <view data-id="{{item.id}}" class="toPay leftButton" style="right:{{role==2?32:32}}rpx;background-color:#{{role==2?'108EE9':'108EE9'}};color:#{{role==2?'FFFFFF':'FFFFFF'}}" wx:if="{{item.type==10 && (role==0)&&item.type!=100}}" catchtap="shouhuo">
          收货
        </view>
        <!-- 添加备注,five为true显示 -->
        <view data-id="{{item.id}}" data-note2="{{item.note2}}" class="toPay beizhu" style="right:32rpx;background-color：108EE9;color:#FFFFFF}}" wx:if="{{five}}" catchtap="beizhuBtn">
          预警备注
        </view>
