        </view>
        <view data-id="{{item.id}}" class="toPay leftButton" style="right:{{role==2?32:32}}rpx;background-color:#{{role==2?'108EE9':'108EE9'}};color:#{{role==2?'FFFFFF':'FFFFFF'}}" wx:if="{{item.type==8 && (role==1||role==2)&&item.type!=100}}" catchtap="filePost">
