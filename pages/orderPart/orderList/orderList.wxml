        </view>
        <!-- 客户查看码单 type8 客户0 -->
        <view data-mdurl="{{item.mdUrl}}"  class="toPay leftButton" style="right:{{role==2?32:32}}rpx;background-color:#{{role==2?'108EE9':'108EE9'}};color:#{{role==2?'FFFFFF':'FFFFFF'}}" wx:if="{{item.type==8 && (role==0||role==2)&&item.type!=100}}" catchtap="madanShow">
          查看码单
        </view>
        <!-- 成品出库 type9 仓管3 -->
        <view data-id="{{item.id}}" class="toPay leftButton" style="right:32rpx;background-color:#{{role==2?'108EE9':'108EE9'}};color:#{{role==2?'FFFFFF':'FFFFFF'}}" wx:if="{{item.type==9 && (role==3||role==2)&&item.type!=100}}" catchtap="chengpinChuhuo">
