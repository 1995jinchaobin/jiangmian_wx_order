        </view> -->
        <!-- 上传码单 type8 业务员1 -->
        <view data-id="{{item.id}}" data-mdurl="{{item.mdUrl}}" class="toPay leftButton" style="right:220rpx;background-color:#{{role==2?'108EE9':'108EE9'}};color:#{{role==2?'FFFFFF':'FFFFFF'}}" wx:if="{{item.type==8 && (role==1||role==2)&&item.type!=100}}" catchtap="madanSure">
