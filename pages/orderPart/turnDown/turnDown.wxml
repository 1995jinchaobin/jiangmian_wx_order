<!--pages/orderPart/turnDown/turnDown.wxml-->
<view>
  <view class="turnDownTitle">驳回理由</view>
  <view class="turnDownDetail">请详细填写您的驳回理由</view><tabBar selected="{{1}}"></tabBar>
  <textarea class="noteArea" value="{{password}}"
      bindinput="bindNoteInput"></textarea>
  <!-- 驳回按钮 -->
  <view class="submitBtn"  bindtap="sureSubmit">立即提交</view>
</view>
