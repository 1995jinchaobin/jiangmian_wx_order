.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  background: rgb(255, 255, 255);
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
  z-index: 11111;
}
.tab-bar-content{
  width: 730rpx;
  display: flex;
}
.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.tab-bar-item cover-image{
  margin-top: 12rpx;
  /* margin-left: -10rpx; */
}
.cover-image1{
  width: 36rpx;
  height: 36rpx;
}
.cover-image2{
  width: 36rpx;
  height: 34rpx;
}
.cover-image3,.cover-image5{
  width: 38rpx;
  height: 36rpx;
}
.cover-image4{
  width: 36rpx;
  height: 34rpx;
}
.tab-bar-item cover-view {
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: rgb(51, 51, 51);
  margin-top: 5px;
  margin-bottom: 12rpx;
}
.yujingNum{
  position: absolute;
  right: 70rpx;
  top: 8rpx;
  color: #fff;
  font-size: 0.5rem;
  background-color: red;
  width: 1rem;
  height: 1rem;
  border-radius: 50%;
}
